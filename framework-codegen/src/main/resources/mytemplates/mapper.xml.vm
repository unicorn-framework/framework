 </sql>

 <!-- 通用查询结果列 -->
 <sql id="Base_Column_List">
     ${table.fieldNames}
 </sql>
 <!--根据Example查询 -->
 <select id="selectListByExample" resultMap="BaseResultMap" parameterType="${entityContext.classImportPath}Example" >
    select
    <if test="distinct" >
      distinct
    </if>
    <include refid="Base_Column_List" />
    from ${table.name}
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
    <if test="orderByClause != null" >
      order by ${orderByClause}
    </if>
    <if test="limit != null" >
      limit  ${limit}
    </if>
 </select>

 <!-- 根据主键查询 -->
 <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="${table.primaryTableField.columnType.javaType}" >
    select 
    <include refid="Base_Column_List" />
    from ${table.name}
    where ${table.primaryTableField.name} = #{${table.primaryTableField.propertyName},jdbcType=${table.primaryTableField.columnType.jdbcType}}
 </select>
  
 <!-- 根据主键列表查询list--> 
<select id="selectListByIds" resultMap="BaseResultMap" parameterType="java.util.List" >
    select 
    <include refid="Base_Column_List" />
    from ${table.name}
    <where>
      and ${table.primaryTableField.name} in
     <foreach item="item" index="index" collection="list" open="(" separator="," close=")">  
      #{item}  
     </foreach>  
    </where>
 </select>
  
 <!-- 查询list--> 
<select id="selectList" resultMap="BaseResultMap" parameterType="${entityContext.classImportPath}" >
    select 
    <include refid="Base_Column_List" />
    from ${table.name}
    <where>
      #foreach($field in ${table.fields})
      #if(!${field.keyFlag})
      
      <if test="${field.propertyName} != null" >
        and ${field.name}=#{${field.propertyName},jdbcType=${field.columnType.jdbcType}}
      </if>
      #end
    #end 
    </where>
 </select>
 <!--根据Example查询记录数 --> 
 <select id="countByExample" parameterType="${entityContext.classImportPath}Example" resultType="java.lang.Integer" >
    select count(1) from ${table.name}
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
 </select>


<!-- 查询单条记录--> 
<select id="selectOne" resultMap="BaseResultMap" parameterType="${entityContext.classImportPath}" >
