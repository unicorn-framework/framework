    from ${table.name}
    <where>
      #foreach($field in ${table.fields})
      #if(!${field.keyFlag})

      <if test="${field.propertyName} != null" >
        and ${field.name}=#{${field.propertyName},jdbcType=${field.columnType.jdbcType}}
      </if>
      #end
    #end 

    </where>
    limit 2
</select>  
  
